RewriteEngine on
Options +FollowSymLinks
RewriteCond %{HTTP_HOST} ^euripedesbarsanulfo.org.br [NC]
RewriteRule ^(.*)$ https://www.euripedesbarsanulfo.org.br/$1 [R=301,L] 
RewriteRule ^texto/([a-z0-9-]+)-([0-9-]+)/?$ texto.php?id=$2 [NC]
RewriteRule ^/inscricao/escola-de-estudos-espiritas inscricao.php?id=1 [NC]